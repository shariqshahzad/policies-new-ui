{
  "name": "dashboard-template-spa",
  "version": "3.2.0",
  "description": "Boilerplate for starting new React projects for Qiwa",
  "main": "index.js",
  "scripts": {
    "build": "tsc --pretty --noEmit && webpack --config scripts/build.ts --node-env=production",
    "webpack-start": "webpack serve --config scripts/start.ts --node-env=development",
    "start": "npm run create-config-json && npm run webpack-start",
    "run-mock-server": "cross-env HTTPS=false APP_API_MOCK=true npm run webpack-start",
    "start:mock": "npm run create-config-json-mock-server && npm run run-mock-server",
    "test:unit": "node --no-compilation-cache -r ts-node/register scripts/test.ts --maxWorkers=25%",
    "test:clear-cache": "jest --clearCache",
    "coverage": "npm run test:unit -- --coverage",
    "format": "prettier --write './src/**/*.{ts,tsx}'",
    "prepare": "npx husky install",
    "lint": "eslint './**/*.{ts,tsx}'",
    "lint:fix": "npm run lint -- --fix",
    "type-check": "tsc --pretty --noEmit",
    "pre-commit": "lint-staged",
    "generator": "plop",
    "translations": "ts-node -T --skip-project ./scripts/translations-babelsheet/fetch-translations.ts && npm run generate-messages-keys-type",
    "translations:ditto": "source .env.local && ts-node -T --skip-project ./scripts/translations-ditto/transform-translations.ts $I18N_DICTIONARY_SRC && prettier --loglevel silent --write ./src/app/translations/i18n/generated/*",
    "create-config-json": "ts-node -T --skip-project ./scripts/create-config-from-env/create-config.ts",
    "create-config-json-mock-server": "ts-node -T --skip-project ./scripts/create-config-from-env/create-config.ts  -- --mock",
    "generate-messages-keys-type": "ts-node -T --skip-project ./scripts/generate-messages-keys-type/generate-messages-keys-type.ts",
    "cleanup-examples": "ts-node -T --skip-project cleanupExamples/cleanup.ts"
  },
  "engines": {
    "node": "18"
  },
  "lint-staged": {
    "!(*config).{js,jsx,ts,tsx}": [
      "eslint --fix"
    ],
    "src/**/*.{ts,tsx}": [
      "bash -c 'npm run type-check'"
    ],
    "*.{json}": [
      "prettier --write"
    ],
    "*.{css,pcss,scss}": [
      "prettier --write"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "author": "Qiwa",
  "license": "UNLICENSED",
  "devDependencies": {
    "@axe-core/react": "^4.7.3",
    "@babel/core": "7.23.2",
